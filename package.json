{
  "name": "vite-bootstrap",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 3433",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "type": "echo \"Running Type check\" ; tsc -p tsconfig.prod.json --noEmit --resolveJsonModule",
    "lint": "echo \"Running ESLint\" ; eslint --cache --ext js,ts,tsx .",
    "test": "echo \"Running Vitest\" ; NODE_ENV=test vitest",
